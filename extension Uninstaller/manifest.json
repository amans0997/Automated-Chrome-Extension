{
    "manifest_version": 2,
    "name": "Extension Uninstaller",
    "version": "1.0",
    "icons": {
        "16": "icon.png"
    },
    "permissions": [
        "management"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}